	~	Dmnl
	~	Weight showing high impact of voting role on politician role.
	|

Rp=Scale parameter * 
	
	IF THEN ELSE(Case switch=1 :OR: Case switch=3 :OR: Case switch=3.1 :OR: Case switch=\
		3.2,
	SQRT(DELAY1(Rv, 2*TIME STEP)) * (Spw * Ipw + (1-Spw) * 0.5),
	
	IF THEN ELSE(Case switch=2 :OR: Case switch=5 ,
	SQRT(DELAY1(Rv, 2*TIME STEP)) * (Dynamic Spw * Ipw + (1-Dynamic Spw) * 0.5),
	
	IF THEN ELSE( Case switch=4,
	IF THEN ELSE(Dynamic Svw < 0.5,
	SQRT(DELAY1(Rv, 2*TIME STEP)) * (Spw * Ipw + (1 - Spw) * 0.5), (Scale parameter+∂)\
		 * SQRT(DELAY1(Rv, 2*TIME STEP)) * (Spw * Ipw +
	 (1 - Spw) * 0.5)
	), 0)))
	~	Dmnl
	~	Variable representing the politician role. We use a default value of 0.5 \
		for the male impact so that we only have to vary the exogenous female \
		impact.
	|

E=IF THEN ELSE ( Case switch=1 :OR: Case switch=2 :OR:Case switch=4 :OR: Case switch=\
		5 , 
	( 1/SQRT(MAX (Rc,1)) + 2/SQRT(MAX(Rf,1)) ) * E0 ,
	
	IF THEN ELSE ( Case switch=3, 0.774*( 1/SQRT(MAX(Rc,1)) + 2/SQRT(MAX(Rf,1)) ) * E0 ,\
		  
	
	IF THEN ELSE ( Case switch=3.1 , 0.915* ( 1/SQRT(MAX(Rc,1)) + 2/SQRT(MAX(Rf,1)) ) * \
		E0, 
	
	IF THEN ELSE ( Case switch=3.2, 0.845*( 1/SQRT(MAX(Rc,1)) + 2/SQRT(MAX(Rf,1)) ) * E0\
		 ,  
	
	0 ))))
	~	tons of co2/Year
	~	Annual emissions level
	|

Y0= INITIAL(Y)
	~	USD
	~	Initial value of income gap
	|

Rc=Scale parameter * 
	
	IF THEN ELSE(Case switch=1 :OR: Case switch=3 :OR: Case switch=4 :OR: Case switch=3.1\
		 :OR: Case switch=3.2 , 
	SQRT(SQRT(DELAY1(Rp, 2*TIME STEP)) * SQRT(DELAY1(Rl, 1*TIME STEP))) * (Scw * Icw + (\
		1-Scw) * 0.5),
	
	IF THEN ELSE(Case switch=2,
	SQRT(SQRT(DELAY1(Rp, 2*TIME STEP)) * SQRT(DELAY1(Rl, 1 *TIME STEP))) * (Dynamic Scw \
		* Icw + (1-Dynamic Scw) * 0.5), 
		 
	IF THEN ELSE( Case switch=5 ,
	SQRT(SQRT(DELAY1(Rp, 2*TIME STEP)) * SQRT(DELAY1(Rl, 1*TIME STEP))) * (Dynamic Scw *\
		 Icw* Y0/Y + (1-Dynamic Scw) * 0.5 ) , 
	
	0 )))
	~	Dmnl
	~	Variable representing the consumer role. We use a default value of 0.5 for \
		the male impact so that we only have to vary the exogenous female impact.
	|

Dynamic Svw=IF THEN ELSE(Time<=5, 0.1,
			IF THEN ELSE(Time<=15, 0.1 + 0.4*(Time-5)/Transition time, 0.5))
	~	Dmnl
	~	Exogenous parameter representing the dynamic share of women within the \
		voting role.
	|

Dynamic Scw=IF THEN ELSE(Time<=15, 0.1,
			IF THEN ELSE(Time<=25, 0.1 + 0.4*(Time-15)/Transition time, 0.5))
	~	Dmnl
	~	Exogenous parameter representing the dynamic share of women within the \
		consumer role.
	|

Dynamic Slw=IF THEN ELSE(Time<=10, 0.1 + 0.4*Time/Transition time , 0.5)
	~	Dmnl
	~	Exogenous parameter representing the dynamic share of women within the \
		lifestyle-influencer role.
	|

Rf=Scale parameter * 
	
	IF THEN ELSE(Case switch=1 :OR: Case switch=3 :OR: Case switch=4 :OR: Case switch=3.1\
		 :OR: Case switch=3.2, 
	SQRT(SQRT(DELAY1(Rp, 5*TIME STEP)) * SQRT(DELAY1(Rc, 1*TIME STEP))) * (Sfw * Ifw + (\
		1-Sfw) * 0.5),
	
	IF THEN ELSE(Case switch=2 :OR: Case switch=5 ,
	SQRT(SQRT(DELAY1(Rp, 5*TIME STEP)) * SQRT(DELAY1(Rc, 1*TIME STEP))) * (Dynamic Sfw *\
		 Ifw + (1-Dynamic Sfw) * 0.5), 0))
	~	Dmnl
	~	Variable representing the role of firm-manager. We use a default value of \
		0.5 for the male impact so that we only have to vary the exogenous female \
		impact.
	|

Rl=Scale parameter * 
	
	IF THEN ELSE(Case switch=1 :OR:  Case switch=4 :OR: Case switch=3.1 ,
	(Slw * Ilw + (1-Slw) * 0.5),
	
	IF THEN ELSE(Case switch=2 :OR: Case switch=5,
	(Dynamic Slw * Ilw + (1-Dynamic Slw) * 0.5),
	
	IF THEN ELSE(Case switch=3 , 
	SQRT(DELAY1(CE/(MAX(CB-CE,1)), 3*TIME STEP)) * (Slw * Ilw + (1-Slw) * 0.5),
	
	IF THEN ELSE( Case switch=3.2 , 
	SQRT(DELAY1(CE/(MAX(CB-CE,1)), 3*TIME STEP)) * (Slw * Ilw + (1-Slw) * 0.5),
	
	 0))))
	~	Dmnl
	~	Variable representing the role of social influence on consumer behaviour \
		(lifestyle-influencer role). We use a default value of 0.5 for the male \
		impact so that we only have to vary the exogenous female impact.
	|

Ro=Scale parameter * 
	
	IF THEN ELSE(Case switch=1 :OR:  Case switch=4 :OR: Case switch=3.2 ,
	(Sow * Iow + (1-Sow) * 0.5),
	
	IF THEN ELSE(Case switch=2 :OR: Case switch=5,
	(Dynamic Sow * Iow + (1-Dynamic Sow) * 0.5),
	
	IF THEN ELSE(Case switch=3  , 
	SQRT(DELAY1(CE/(MAX(CB-CE,1)), 3*TIME STEP)) * (Sow * Iow + (1-Sow) * 0.5),
	
	IF THEN ELSE(Case switch=3.1 , 
	SQRT(DELAY1(CE/(MAX(CB-CE,1)), 3*TIME STEP)) * (Sow * Iow + (1-Sow) * 0.5),
	
